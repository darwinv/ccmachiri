/*----start-consulation-----*/
.consulation{
	padding:3em 0;
}
.c-head h3 {
	width: 28.3%;
}
.c-head span{
	margin-left: 1.3em;
}

.consulation-left {
	position:relative;
	float:right;
	width:42%;
	margin:0%;
	right:3%;
	-webkit-transition:all 1s;
	-moz-transition:all 1s;
	transition:all 1s;
	margin-bottom:4%;
	padding:2% 1%;
	min-height: 600px;
	height: auto;
}
.col-cot2 {
	position:relative;
	text-align: center;
	width:100%;
	height:550px;
	padding:0%;
	background-color:#585858;
	border:1px solid #EEEEEE;
	border-radius:1vw;
	margin:auto;
	-webkit-box-shadow:0vw 0vw 2vw #000000;
	-moz-box-shadow:0vw 0vw 2vw #000000;
	box-shadow:0vw 0vw 2vw #000000;
}

.consulation-left h4{
	font-size: 2em;
	color: #000;
	font-weight: 400;
	width: 60%;
	line-height: 1.5em;
}
.consulation-left p{
	color: #222;
	font-family: 'Oxygen', sans-serif;
	line-height: 1.8em;
	font-size: 0.9em;
	width: 65%;
}
/*-----*/
#tags
{
	-webkit-box-shadow:0vw 0vw 2vw #000000;
	-moz-box-shadow:0vw 0vw 2vw #000000;
	box-shadow:0vw 0vw 2vw #000000;
}
#con2
{
	background:rgba(0, 0, 0, 0.6);
}
.container1
{
	width:70%;
	margin:auto;
	position:relative;
}
.container1 p , .container1 , .container1 p span
{
	color:#FFFFFF !important;
	font-size:17px !important;
	text-align:justify !important;
}
.col-cot{
	position:relative;
	width:100%;
	min-height:100%;
	padding:0%;
	padding-bottom:20%;
	margin-left:1vw;
	background-color:#D8D8D8;
	border:1px solid #EEEEEE;
	-webkit-box-shadow:0vw 0vw 2vw #000000;
	-moz-box-shadow:0vw 0vw 2vw #000000;
	box-shadow:0vw 0vw 2vw #000000;
}
.col-cot6{
	position:relative;
	width:80%;
	min-height:100%;
	padding:1% 4%;
	padding-bottom:5%;
	background-color:#D8D8D8;
	border:1px solid #EEEEEE;
	margin:auto;
	-webkit-box-shadow:0vw 0vw 2vw #000000;
	-moz-box-shadow:0vw 0vw 2vw #000000;
	box-shadow:0vw 0vw 2vw #000000;
	overflow:hidden;
}
.col-cot4{
	position:relative;
	width:60%;
	padding:5%;
	left:50%;
	margin-left:-30%;
	background-color:#D8D8D8;
	border:1px solid #EEEEEE;
	-webkit-box-shadow:0vw 0vw 2vw #000000;
	-moz-box-shadow:0vw 0vw 2vw #000000;
	box-shadow:0vw 0vw 2vw #000000;
}
.col-cot3{
	position:relative;
	width:40%;
	min-height:660px;
	padding:0%;
	background-color:#D8D8D8;
	border:1px solid #EEEEEE;
	border-right:none;
	max-width:600px;
	float:left;
	left:50%;
	margin-left:-40%;
	margin-right:0;
	z-index:3;
	display:inline-block;
	-webkit-box-shadow:-0.5vw 0vw 2vw #000000;
	-moz-box-shadow:-0.5vw 0vw 2vw #000000;
	box-shadow:-0.5vw 0vw 2vw #000000;
}
.col-cot5{
	position:relative;
	width:37%;
	min-height:660px;
	padding:0%;
	background-color:#D8D8D8;
	border:1px solid #EEEEEE;
	border-left:none;
	max-width:600px;
	float:left;
	left:50%;
	margin-left:-30%;
	margin-left:-1%;
	padding-left:5%;
	z-index:2;
	display:inline-block;
	-webkit-box-shadow:0vw 0vw 2vw #000000;
	-moz-box-shadow:0vw 0vw 2vw #000000;
	box-shadow:0vw 0vw 2vw #000000;
}
.sub-1
{
	position:relative;
	float:left;
	width: 100%;
	min-height:250px;
	border-bottom: 3px solid #7F7F7F;
	word-wrap: break-word;
	padding: 10px 0% 5px;
	padding-right:5%;
}
.sub-1 p span , .menu_footer p span
{
	margin:0;
	margin-left:20px;
	display:block;
}
.sub-2
{
	position:relative;
	float:left;
	width: 100%;
	min-height:300px;
	word-wrap: break-word;
	padding: 5px 0% 5px;
	padding-right:4%;
}
.sub-2 strong , .sub-2 p 
{
	color:#000000 !important;
}
.sub-2 p 
{
	font-size:12px !important;
}
.sub-3 
{
	position:relative;
	width:98%;
	visibility:visible;
	min-height:300px;
	padding:5% 4%;
	background-color:#D8D8D8;
	border:1px solid #EEEEEE;
	border-right:none;
	float:left;
	left:0%;
	margin-right:0;
	z-index:3;
	display:none;
	-webkit-box-shadow:0vw 0vw 2vw #000000;
	-moz-box-shadow:0vw 0vw 2vw #000000;
	box-shadow:0vw 0vw 2vw #000000;
}
.title-plan{
	position:relative;
	width:100%;
	padding:3% 0;
	text-align:center;
}

.title-plan h2{
	position:relative;
	color:#FFFFFF;
	font-size:50px;
}
.cont-plan {
	position:relative;
	width:100%;
	text-align:center;
	padding:5px 10%;
	color:#FFFFFF;
	font-size:20px;
	font-weight:300;
}
.footer-plan {
	position:absolute;
	width:100%;
	min-height:5%;
	height:auto;
	text-align:center;
	bottom:0%;
	padding-bottom:10%;
	margin-bottom:0;
	float:bottom;
}
.title-cotiza {
	position:relative;
	width:100%;
	border-bottom:4px solid #7F7F7F;
	padding:1.5% 5%;
	padding-left: 0;
	margin-bottom:30px;
	text-align:right;
}
.title-empresa{
	position:relative;
	width:100%;
	border-bottom:4px solid #7F7F7F;
	padding:1.5% 5%;
	padding-left: 0;
	margin-bottom:30px;
	text-align:left;
}
.title-cotiza h2 font{
	font-size:25px !important;
}
.title-empresa h2 font{
	font-size:25px !important;
}
.title-verificar {
	position:relative;
	width:100%;
	border-bottom:4px solid #7F7F7F;
	padding:1.5% 5%;
	padding-left: 0;
	margin-bottom:30px;
	text-align:left;
}
.title-contact {
	position:relative;
	width:100%;
	border-bottom:4px solid #7F7F7F;
	padding:1.5% 3%;
	margin-bottom:30px;
	text-align:left;
}
.title-contact h2 font{
	font-size:25px !important;
}
.footer-cotiza {
	position:absolute;
	width:100%;
	min-height:5%;
	height:auto;
	background-color:#4D4D4D;
	text-align:center;
	bottom:0%;
	margin-top:5%;
	margin-bottom:0;
	float:bottom;
	padding:5px 0;
}
.footer-mensage {
	position:absolute;
	width:100%;
	min-height:5%;
	height:auto;
	background-color:#4D4D4D;
	text-align:center;
	bottom:0%;
	margin-top:5%;
	margin-bottom:0;
	float:bottom;
	padding:5px 0;
}
.footer-mensage font
{
	color:#FFFFFF;
	font-size:1.2em;
	margin-left: 2%;
}
.footer-mensage input[type="checkbox"]
{
	-webkit-transform: scale(1.7);
	-moz-transform: scale(1.7);
	transform: scale(1.7);
	cursor: pointer;
	margin:0 5px;
	width:2vw;
	max-width:2vw;
	margin-left:15px;
	margin-right:1vw;
	border:none;
	outline:none;
}
.footer-cotiza .bt , .footer-mensage .bt , footer-plan .bt {
	position:relative;
	padding:10px 40px;
	border-radius:2vw;
	background-color:#4D4D4D;
	border:2px solid #FFFFFF;
	color:#FFFFFF;
	font-size:15px;
	font-weight:bold;
	margin:2%;
	cursor:pointer;
	max-width:200px!important;
}
.inp4 button[type="button"].button {
	position:relative;
	padding:10px 40px;
	border-radius:2vw;
	background-color:#4D4D4D;
	border:2px solid #FFFFFF;
	color:#FFFFFF;
	font-size:15px;
	font-weight:bold;
	margin:2%;
	cursor:pointer;
}


.bt:hover  {
	background-color:#141414;
}
.footer-cotiza .bt:active , .bt:focus , .footer-mensage .bt:active {
	outline:none
}
.consulation-right {
	margin:0% 2%;
	margin-left:0%;
	-webkit-transition:all 1s;
	-moz-transition:all 1s;
	transition:all 1s;
	border-right:3px solid #7F7F7F;
	margin-bottom:4%;
	padding:2% 2%;
	padding-right:6%;
	height: 590px;
	overflow: hidden;
	right:0%;
}
.contact {
	margin:0% 2%;
	-webkit-transition:all 1s;
	-moz-transition:all 1s;
	transition:all 1s;
	margin-bottom:4%;
	padding:2% 1%;
	min-height: 400px;
	height: auto;
}
.form div.inp
{
	position:relative;
	width:49%;
	padding:3px;
	padding-left:5%;
	float:left;
}

.form div.inp11
{
	/*float:left;*/
	padding:3px;
	padding-left:5%;
	width:55%;
}
.form div.inp2
{
	position:relative;
	width:100%;
	padding:3px;
	padding-left:2%;
	padding-right:0%;
	float:left;
}
.col-cot .inp {
	height:80px;
}

.fields
{
	position:relative;
	width: 100% !important;
}

