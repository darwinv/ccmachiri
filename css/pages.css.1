	min-width:90%;
	width:90%;
}
.col-cot4 .inp input , .col-cot4 .inp select {
	min-width:95%;
	width:95%;
}

.col-cot4 .inp11 input , .col-cot4 .inp11 select {
	min-width:95%;
	width:95%;
}
.col-cot .inp input.date {
	width:86% ;
}

.col-cot .inp11 input.date {
	width:80% ;
}
.ui-datepicker-trigger
{
	width:8%;
	height:18px;
	margin:0 3%;
